{
  "name": "dwo/client",
  "description": "client collection to switch easily between real and test clients",
  "keywords": ["client", "guzzle", "test"],
  "homepage": "https://github.com/davewwww",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Dave Www",
      "email": "davewwwo@gmail.com",
      "homepage": "https://github.com/davewwww"
    }
  ],
  "require": {
    "php": ">=5.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4",
    "dwo/tracedump": "@dev"
  },
  "suggest": {
    "symfony/http-kernel": "To use KernelLogClient",
    "guzzlehttp/guzzle": "To use GuzzleClient",
    "symfony/process": "To use PhantomJsClient"
  },
  "autoload": {
    "psr-0": {"Dwo\\Client": "src/"}
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
